<!DOCTYPE html>
<html>
  <head>
    <title>PapaluCa</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>

    <!-- log in check -->
    <% if logged_in? %>
      <p> logged in as <%= current_user.name %> </p>
    <% end %>
  </body>
</html>
